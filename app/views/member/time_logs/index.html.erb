<div class="container">
  <h1 style="margin-top:20px">Time Logs</h1>

  <%= link_to 'Add Time Log', new_member_time_log_path, class: 'btn btn-primary', style: 'margin-left:1170px' %>

  <table class="table ", style="margin-top:20px">
    <thead>
      <tr>
        <th>Project Name</th>
        <th>Start Time</th>
        <th>End Time</th>
        <th>Action</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody class="table">
      <% @logs.each do |log| %>
        <tr>
          <td><%= log.project.title %></td>
          <td><%= log.start_time.to_formatted_s(:time) %></td>
          <td><%= log.end_time.to_formatted_s(:time) %></td>
          <td><%= link_to 'Show', member_time_log_path(log), class: "btn btn-info" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

